{
  "name": "@spudly/scripture.study",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "UNLICENSED",
  "author": "Stephen Sorensen <shuoink@gmail.com>",
  "main": "index.js",
  "scripts": {
    "build": "npm run codegen && npm run build:server && npm run build:client",
    "build:client": "(rm stats.json || true) && (rm -rf public/js || true) && (rm -rf public/css || true) && cross-env NODE_ENV=production webpack --config=build/webpack.config.js --json > stats.json",
    "build:server": "rm -rf build && tsc",
    "codegen": "npm run codegen:meta",
    "codegen:meta": "ts-node -r dotenv/config scripts/meta.ts | prettier --stdin-filepath src/meta.ts > src/meta.ts",
    "depcheck": "node node_modules/depcheck/bin/depcheck.js --ignores=\",@commitlint/*,@types/*,prettier,pretty-quick,sort-package-json,typescript,autoprefixer,tailwindcss,postcss-import,webpack-cli,pg,pg-hstore,*-loader,babel-*,dotenv\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "seed": "ts-node -r dotenv/config scripts/seed.ts | pino-pretty",
    "start": "cross-env NODE_ENV=production node build/index.server.js",
    "start:dev": "cross-env NODE_ENV=development npm run start:dev:server | pino-pretty",
    "start:dev:server": "cross-env NODE_ENV=development npm run codegen && ts-node -r dotenv/config src/index.server.dev.tsx",
    "start:test": "cross-env NODE_ENV=test nyc npm run start:dev:server",
    "test": "rm -rf .nyc-output && rm -rf coverage && npm run test:unit && npm run test:integration",
    "test:integration": "start-server-and-test start:test http://localhost:3000 test:integration:run",
    "test:integration:open": "cypress open",
    "test:integration:run": "cypress run",
    "test:unit": "jest --coverage --forceExit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,htm,html,css}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json",
      "prettier --write"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "loose": true,
          "useBuiltIns": "usage",
          "corejs": 3,
          "shippedProposals": true,
          "include": [
            "@babel/plugin-proposal-nullish-coalescing-operator",
            "@babel/plugin-proposal-optional-chaining"
          ]
        }
      ],
      "@babel/preset-react"
    ]
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "last 2 safari versions"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "proseWrap": "always",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "@spudly",
    "rules": {
      "no-confusing-arrow": "off",
      "jsx-a11y/no-noninteractive-element-to-interactive-role": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/",
      "/build/"
    ]
  },
  "nyc": {
    "cache": false,
    "exclude": [
      "**/*.test.*",
      "**/*.stories.*"
    ],
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.{ts,tsx}"
    ],
    "reporter": [
      "text",
      "json",
      "lcov"
    ]
  },
  "dependencies": {
    "@meronex/icons": "^4.0.0",
    "@reach/auto-id": "^0.12.1",
    "@spudly/pushpop": "^2.1.3",
    "body-parser": "^1.19.0",
    "bowser": "^2.11.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^5.0.0-alpha.8",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "history": "^4.9.0",
    "node-fetch": "^3.1.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pino": "^6.11.0",
    "pino-http": "^5.3.0",
    "pino-pretty": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.6.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "sequelize": "^6.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/code-coverage": "^3.9.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@spudly/eslint-config": "^8.0.1",
    "@testing-library/cypress": "^7.0.3",
    "@types/autoprefixer": "^9.7.2",
    "@types/classnames": "^2.2.11",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cssnano": "^4.0.0",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/google.analytics": "0.0.41",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/passport": "^1.0.5",
    "@types/passport-auth0": "^1.0.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/pino": "^6.3.5",
    "@types/pino-http": "^5.0.6",
    "@types/postcss-import": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "browserslist": "^4.16.1",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "cypress": "^6.3.0",
    "depcheck": "^1.3.1",
    "eslint": "^7.18.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.4",
    "nyc": "^15.1.0",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-refresh": "^0.9.0",
    "sort-package-json": "^1.48.1",
    "start-server-and-test": "^1.11.7",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
